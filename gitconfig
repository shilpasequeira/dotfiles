[include]
  path = ~/.gitconfig.local
[push]
  default = current
[alias]
  ci = commit -v
  cia = commit -v -a
  co = checkout
  df = diff --color
  dt = difftool
  rb = rebase
  st = status
  rl = reflog
  br = branch
  aliases = config --get-regexp alias.*
  remove-untracked-files = clean -f
[color]
  ui = true
[color "status"]
  added = green
  changed = yellow
  untracked = magenta
